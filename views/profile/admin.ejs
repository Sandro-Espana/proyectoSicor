        <button type="button" class="btn btnmenu" onclick="formPqrsAdmin()">
          Pqrs
        </button>
        <button type="button" class="btn btnmenu" onclick="formCartelera()">
          CARTELERA
        </button>
        <button type="button" class="btn btnmenu" onclick="formTarea()">
          Calendario
        </button>
        <button type="button" class="btn btnmenu" onclick="formSanciones()">
          Sanciones
        </button>
        <button type="button" class="btn btnmenu" onclick="formRepositorio()">
          Documentos
        </button>
        <button
          type="button"
          class="btn btnmenu"
          onclick="formEncuestasPreguntas()"
        >
          encuestas
        </button>
        <button type="button" class="btn btnmenu" onclick="formZonasComunes()">
          zonas comunes
        </button>
      </div>
      <div class="search-container">
        <input type="text" id="searchInput" onkeyup="doSearch()" placeholder="Buscar...">
      </div>
      <div id="container-table" class=" "></div>
      <br>
      <br>
      <br>
    </main>
    <footer><%- include('footer.ejs') %></footer>
    <script src="js/swalDos.js"></script>
    <script src="/js/admin/formProveedor.js"></script>
    <script src="/js/admin/formPqrsAdmin.js"></script>
    <script src="/js/admin/formCartel.js"></script>
    <script src="js/admin/formTarea.js"></script>
    <script src="/js/admin/formSanciones.js"></script>
    <script src="/js/admin/formReposit.js"></script>
    <script src="/js/admin/formEncuesPreg.js"></script>
    <script src="js/admin/formZonComAdm.js"></script>
  </body>
  <script>
    window.onload = function(){
      document.getElementById('searchInput').style.display = 'none';
}
  </script>
</html>
